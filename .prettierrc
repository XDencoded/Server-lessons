{
	"trailingComma": "none",
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"useTabs": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/routes/",
    "/middleware/",
    "^../(.*)",
    "^./(.*)"
  ],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
